{% extends '::header.html.twig' %}
{% block title %}Administration - Photos{% endblock %}
{% block main_title %}
    <header>
        <div class="wrapper">
            <h1 class="title">Voir une photo</h1>
        </div>
    </header>
{% endblock %}
{% block content %}
    <div id="content">
        <div class="wrapper">

    <table class="record_properties">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('admin_photo') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('admin_photo_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
        </div>
    </div>
{% endblock %}
